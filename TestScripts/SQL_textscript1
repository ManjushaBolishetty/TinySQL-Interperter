SELECT * FROM inventory
SELECT * FROM inventory ORDER BY sku
SELECT * FROM inventory WHERE sku = 101
SELECT * FROM inventory WHERE sku = 132 OR quantity > 99
SELECT * FROM inventory WHERE quantity > 70
SELECT * FROM inventory WHERE ( sku * 30 + quantity * 20 + sizeid * 5 ) / 100 = 100
