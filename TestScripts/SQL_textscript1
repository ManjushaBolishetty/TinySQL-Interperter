SELECT * FROM OrderDetails
SELECT * FROM OrderDetails WHERE NOT quantity <  2
SELECT * FROM OrderDetails WHERE quantity + sku = 134
SELECT * FROM OrderDetails WHERE orderid < 1300 AND sku = 105
SELECT * FROM OrderDetails WHERE size = "Medium" AND [ sku > 120 OR orderid > 1350 ] AND NOT (quantity) + 100 < 60
SELECT inventory.sku, inventory.sizeid, OrderDetails.quantity FROM inventory, OrderDetails WHERE inventory.sku = OrderDetails.sku
SELECT * FROM inventory, OrderDetails
DELETE FROM inventory
DELETE FROM OrderDetails
DROP TABLE inventory
DROP TABLE OrderDetails
